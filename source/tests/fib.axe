def fib(n: i32): i32 {
    if n <= 1 {
        return n;
    }

    mut val a: i32 = 0;
    mut val b: i32 = 1;
    mut val i: i32 = 2;

    loop {
        if i > n {
            break;
        }

        mut val temp: i32 = a + b;
        a = b;
        b = temp;

        i = i + 1;
    }

    return b;
}

main {
    mut val i: i32 = 0;
    loop {
        if i > 20 {
            break;
        }

        print "fib(";
        print i;
        print ") = ";
        println fib(i);

        i = i + 1;
    }
}
